Set objShell = CreateObject("WScript.Shell")
Set objWshScriptExec = objShell.Exec("wsl -l -v")
Set objStdOut = objWshScriptExec.StdOut

While Not objStdOut.AtEndOfStream
   strLine = objStdOut.ReadLine
   var utf8bytes = Encoding.UTF8.GetBytes(strLine)
   var win1252Bytes = Encoding.Convert(Encoding.UTF8,Encoding.ASCII,strLine)
   If InStr(strLine,"Arch") Then
       WScript.Echo strLine
   End If
Wend